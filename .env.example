# testdata-ai Configuration
# Copy this file to .env and add your actual keys

# === AI PROVIDER SETTINGS ===

# Default provider to use (openai, anthropic, or custom)
AI_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=sk-proj-your-openai-key-here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=2000

# Anthropic Claude
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_TEMPERATURE=0.7
ANTHROPIC_MAX_TOKENS=2000

# === APPLICATION SETTINGS ===

# Default output format (json, csv, sql)
DEFAULT_OUTPUT_FORMAT=json

# Default number of records
DEFAULT_COUNT=10

# Enable caching (true/false)
CACHE_ENABLED=true
CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO
LOG_FILE=testdata-ai.log

# === CUSTOM CONTEXTS ===

# Directory for user-defined contexts
CUSTOM_CONTEXTS_DIR=./contexts